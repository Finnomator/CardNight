<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <VBox alignment="CENTER">
            <children>
                <Label text="Developed by:" textAlignment="CENTER"/>
                <Label text="Finn" textAlignment="CENTER"/>
                <Label text="Chris" textAlignment="CENTER"/>
                <Label text="Marc" textAlignment="CENTER"/>
                <Label text="David" textAlignment="CENTER"/>
            </children>
        </VBox>
        <Button mnemonicParsing="false" text="Back" GridPane.rowIndex="1"/>
    </children>
</GridPane>
